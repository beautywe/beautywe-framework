{
    "extends": "airbnb-base",
    "globals": {
        "App": true,
        "wx": true,
        "getApp": true,
        "getCurrentPages": true,
        "Page": true,
        "Component": true
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "func-names": ["error"],
        "no-param-reassign": ["error", { "props": false }],
        "max-len": ["error", { "code": 150 }],
        "no-underscore-dangle": 0,
        "new-cap": ["error", { "capIsNewExceptions": ["App", "Page", "Component", "MyPage", "MyComp", "MyApp"] }]
    }
}