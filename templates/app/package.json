{
    "name": "<%= appName %>",
    "version": "<%= version %>",
    "scripts": {
        "dev": "cross-env RUN_ENV=dev ./node_modules/.bin/beautywe run dev",
        "test": "cross-env RUN_ENV=test ./node_modules/.bin/beautywe run test",
        "prod": "cross-env RUN_ENV=prod ./node_modules/.bin/beautywe run prod",
        "commitmsg": "validate-commit-msg",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "commit": "git cz",
        "lint": "git diff --cached --name-only | grep -E '\\.(js|jsx)$' | xargs eslint"
    },
    "dependencies": {
        "@beautywe/core": "^1.0.0",
        "@beautywe/plugin-event": "^1.0.0",
        "@beautywe/plugin-storage": "^1.0.0",
        "@beautywe/plugin-listpage": "^1.0.0",
        "@beautywe/plugin-logger": "^1.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.0.0",
        "@babel/register": "^7.4.0",
        "@beautywe/cli": "^1.0.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-array-includes": "^2.0.3",
        "chalk": "^2.4.1",
        "commitizen": "^2.9.6",
        "cross-env": "^5.2.0",
        "cz-conventional-changelog": "^2.0.0",
        "del": "^3.0.0",
        "eslint": "^5.3.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.16.0",
        "gulp": "^4.0.0",
        "gulp-babel": "^8.0.0",
        "gulp-clean": "^0.4.0",
        "gulp-csso": "^3.0.1",
        "gulp-define-module": "^0.1.5",
        "gulp-imagemin": "^4.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "^0.6.1",
        "gulp-sass": "^3.1.0",
        "gulp-tap": "^1.0.1",
        "gulp-uglify": "^3.0.0",
        "lodash": "^4.17.11",
        "plugin-error": "^1.0.1",
        "pre-commit": "^1.2.2",
        "through2": "^2.0.3",
        "time-buried": "0.0.5",
        "validate-commit-msg": "^2.14.0",
        "webpack": "^4.29.6",
        "webpack-stream": "^5.2.1",
        "fs-extra": "^8.0.0"
    }
}
